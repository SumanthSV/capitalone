# KrishiMitra Environment Configuration

# Required API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Optional API Keys (for enhanced features)
ENAM_API_KEY=your_enam_api_key_here

# Security Keys (generate strong random keys for production)
SECRET_KEY=your_generated_secret_key_here
JWT_SECRET_KEY=your_generated_jwt_secret_key_here

# Database Configuration
DATABASE_URL=sqlite:///crop_data.db

# Application Settings
DEBUG=true
LOG_LEVEL=INFO
DEFAULT_LANGUAGE=hi
CACHE_EXPIRY_HOURS=6

# SMS/Phone Authentication (optional)
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=your_twilio_number

# CORS Settings
ALLOWED_ORIGINS=http://localhost:8000,http://127.0.0.1:8000